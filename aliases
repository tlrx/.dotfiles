
# Unix
alias ll="ls -al"
alias ln="ln -v"
alias mkdir="mkdir -p"

# Git
alias master="git checkout master"
alias reset="git reset --soft HEAD~1"

# Directories
alias es="cd /home/tanguy/Dev/elastic/elasticsearch"
alias es6="cd /home/tanguy/Dev/elastic/elasticsearch-6.x"

# Gradle
alias clean="gradle clean --parallel"
alias precommit="gradle precommit --parallel"
alias assemble="gradle :distribution:archive:zip:assemble --parallel"

